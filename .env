# ===========================================
# Environment Variables for Nabat Application
# ===========================================
# WARNING: Never commit this file with real values to version control!
# Copy this file to .env.local for local development

# Database Configuration
POSTGRES_DB=nabat_db
POSTGRES_USER=nabat_user
POSTGRES_PASSWORD=change_me_in_production

# Development Database (for docker-compose.dev.yml)
DEV_POSTGRES_DB=nabat_db_dev
DEV_POSTGRES_USER=dev_user
DEV_POSTGRES_PASSWORD=dev_password

# JWT Configuration
JWT_SECRET=nabat-jwt-secret-key-min-256-bits-change-this-in-production-2024

# Spring Configuration
SPRING_PROFILES_ACTIVE=prod

